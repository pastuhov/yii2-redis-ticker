{
  "name": "pastuhov/yii2-redis-ticker",
  "description": "Yii2 Redis ticker.",
  "keywords": [
    "redis",
    "timer",
    "ticker",
    "dog pile"
  ],
  "type": "yii2-extension",
  "homepage": "https://github.com/pastuhov/yii2-redis-ticker",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Kirill Pastukhov",
      "email": "kirill@pastukhov.su",
      "homepage": "https://github.com/pastuhov/",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : ">=5.3.0",
    "yiisoft/yii2": "~2.0.0",
    "yiisoft/yii2-redis": "~2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "4.*",
    "scrutinizer/ocular": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "pastuhov\\yii2redisticker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "pastuhov\\yii2redisticker\\tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
